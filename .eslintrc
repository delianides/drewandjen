{
  "parser": "babel-eslint",
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error", { "trailingComma": true }],
    "quotes": [2, "double"],
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "react/jsx-filename-extension": 0,
    "react/no-danger": 0,
    "react/forbid-prop-types": 0
  },
  "env": {
    "browser": true
  },
  "globals": {
    "STATIC": true
  }
}
